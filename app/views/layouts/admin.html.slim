doctype html
html lang="en"
  head
    title Mainichi Spa - Admin
    = stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
  body.admin
    #container.row
      #left_bar.col-lg-2
        .box
          h2.category_title
            = link_to 'Admin', admin_path
